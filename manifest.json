
{
   "name": "The Mudge",
    "description": "Muddy up your unhealthy sites and take back your focus!",
    "version": "1.0.0",
    "manifest_version": 3,
    "permissons": ["tabs"],
    "action": {
        "default_popup": "popup.html"
    },

    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]

    }],

    "permissions": [
        "active_tabs",
        "storage"
    ]
}