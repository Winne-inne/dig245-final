
{
   "name": "The Mudge",
    "description": "Muddy up your unhealthy sites and take back your focus!",
    "version": "1.0.0",
    "manifest_version": 3,
    "permissons": ["tabs"],
    "action": {
        "default_popup": "popup.html"
    },

    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]

    }],


    "web_accessible_resources": [{
            "resources": ["newEnclosure.gif", "siteBook.png"],
            "matches": ["<all_urls>"]
        
    }], 
     
    "permissions": [
        "active_tabs",
        "storage"
    ]
}